# Figma API Configuration
FIGMA_ACCESS_TOKEN=figd_your_token_here

# Optional: Default file key for testing (can also be passed per-request)
# Extract from Figma URL: https://www.figma.com/file/FILE_KEY_HERE/File-Name
# Example: If URL is https://www.figma.com/file/ABC123xyz456/My-Design
#          Then FILE_KEY is: ABC123xyz456
FIGMA_FILE_KEY=

# Server Configuration
LOG_LEVEL=info
API_TIMEOUT_MS=10000

# Rate Limiting
MAX_REQUESTS_PER_MINUTE=60
